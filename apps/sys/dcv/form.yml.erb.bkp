cluster: "irb_cluster"

attributes:
  job_name:
    widget: "text_field"
    value: ""
    label: "Job Name"
  cores:
    widget: "number_field"
    max: 64
    min: 1
    step: 1
    value: 1
    label: "Number of cores"
    help: "Enter a value between 1 and 64"
  memory:
    widget: "number_field"
    max: 512
    min: 1
    step: 1
    value: 1
    label: "Total memory (GB)"
    help: "Enter a value in GB between 1 and 512"

form:
  - job_name
  - bc_num_hours
  - cores
  - memory
